@model MVCDemo.Models.Employee

@{
    ViewBag.Title = "Employees";
}

<h2>Employee Detail</h2>
<table>
    <tr>
        <td>
            Employee Id:
        </td>
        <td>
            @Model.EmployeeId
        </td>
    </tr>
    <tr>
        <td>
            Name:
        </td>
        <td>
            @Model.Name
        </td>
    </tr>
    <tr>
        <td>
            Gender:
        </td>
        <td>
            @Model.Gender
        </td>
    </tr>
    <tr>
        <td>
            City
        </td>
        <td>
            @Model.City
        </td>
    </tr>
</table>
